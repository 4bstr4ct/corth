
import "std.io.corth"


struct Data
{
	int32 value;
};


constproc(void) hello():
	print("Hello, World!\n");
end


constproc(float64) pi():
	return 3.14159265358979323846264338327950;
end


constproc(int32) toInt32(float64 value):
	return (int32)value;
end


proc(int32) main(int32 argc, string[] argv):
	hello();

	Data data;
	data.value = toInt32(pi());

	if data.value == 3:
		print("Rounded PI to int32 is 3\n");
	end

	return 0;
end
